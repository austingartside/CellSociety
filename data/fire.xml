<?xml version="1.0"?>
<simulation name="Fire" author="team19">
	<grid width="20" height="20"/>
	<neighborhood size="5">
		<edge type="constant" value="empty">
		</edge>
	</neighborhood>
	<states>
		<state value="tree" color="#33cc33"></state>
		<state value="burning" color="#ffcc00"></state>
		<state value="empty" color="brown"></state>
	</states>
	<init default="tree">
		<cell state="burning" row="10" col="10"></cell>
	</init>
	<animation framesPerSec="1"></animation>
	<param name="probability" value="0.5"></param>
</simulation>
