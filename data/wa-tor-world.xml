<?xml version="1.0"?>
<simulation name="Wa-Tor World" author="team19">
	<grid width="20" height="20"/>
	<neighborhood size="5">
		<edge type="constant" value="alive">
		</edge>
	</neighborhood>
	<states>
		<state value="fish" color="green" reproductionTime="20"></state>
		<state value="shark" color="black" reproductionTime="20" lifeTime="21"></state>
		<state value="empty" color="white"></state>
	</states>
	<init default="empty">
		<cell state="alive" row="10" col="20"></cell>
		<cell state="alive" row="11" col="20"></cell>
	</init>
	<animation framesPerSec="1"></animation>
	<param name="percentEmpty" value="0.1"></param>
	<param name="sharkToFishRatio" value="0.2"></param>
</simulation>
