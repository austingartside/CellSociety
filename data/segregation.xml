<?xml version="1.0"?>
<simulation name="Segregation" author="team19">
	<grid width="20" height="20"/>
	<neighborhood size="5">
		<edge type="constant" value="alive">
		</edge>
	</neighborhood>
	<states>
		<state value="typeA" color="grey"></state>
		<state value="typeB" color="black"></state>
		<state value="empty" color="white"></state>
	</states>
	<init mode="param">
	</init>
	<animation framesPerSec="1"></animation>
	<param name="percentEmpty" value="0.1"></param>
	<param name="percentTypeA" value="0.1"></param>
	<param name="probability" value="0.1"></param>
</simulation>
